{
  "name": "subscription-service",
  "version": "0.0.1",
  "scripts": {
    "build": "tsc",
    "prebuild": "npm run clean",
    "clean": "rm -rf ./dist",
    "start": "ts-node -r dotenv/config src/SubscriptionServiceServer.ts",
    "watch": "ts-node-dev -r dotenv/config --respawn src/SubscriptionServiceServer.ts",
    "test:unit": "mocha **/*Spec.ts",
    "test:integration": "mocha **/*IT.ts",
    "test": "npm-run-all --parallel test:unit test:integration",
    "test:local": "cross-env MONGODB_URI=mongodb://localhost:27017/subs npm run test",
    "format": "prettier --print-width 120 --write 'src/**/*.ts'"
  },
  "devDependencies": {
    "@types/express": "4.17.8",
    "@types/mocha": "8.0.3",
    "@types/mongodb": "3.5.32",
    "@types/node": "14.14.6",
    "@types/pino": "6.3.9",
    "@types/supertest": "2.0.10",
    "@types/uuid": "8.3.1",
    "@types/validator": "13.6.3",
    "lodash.times": "4.3.2",
    "mocha": "8.4.0",
    "nodemon": "2.0.7",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "supertest": "6.0.1"
  },
  "dependencies": {
    "celebrate": "14.0.0",
    "cross-env": "7.0.2",
    "date-fns": "2.16.1",
    "dotenv": "8.2.0",
    "emittery": "0.7.2",
    "express": "5.0.0-alpha.8",
    "express-list-endpoints": "5.0.0",
    "folktale": "2.3.2",
    "iron_mq": "0.9.3",
    "mongodb": "3.6.3",
    "pino": "6.7.0",
    "uuid": "8.3.1",
    "ts-node": "9.0.0",
    "ts-node-dev": "1.0.0",
    "typescript": "4.0.5",
    "validator": "13.1.17"
  }
}
